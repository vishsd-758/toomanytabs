{
  "manifest_version": 3,
  "name": "Tab Tracker",
  "version": "1.0.0",
  "description": "Track open tabs, search queries, and usage patterns",
  "permissions": [
    "tabs",
    "storage",
    "history",
    "webNavigation"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
